   "source": [
    "select * from PG_TABLES;\n",
    "select * from retale_sales;\n",
    "\n",
    "--3.3 데이터 트랜드 분석\n",
    "-- 소매업과 외식업의 월간 매출 \n",
    "select sales_month, sales from retale_sales rs where kind_of_business = 'Retail and food services sales, total' order by 1;\n",
    "\n",
    "-- 소매업과 외식업의 연간 매출 \n",
    "select date_part('year', sales_month::date) as sales_year, sum(sales) as sales from retale_sales rs \n",
    "where kind_of_business = 'Retail and food services sales, total' group by 1 order by 1 ;\n",
    "\n",
    "-- 서점업, 스츠용품업, 취미/장난감/게임업의 연간매출 \n",
    "select date_part('year', sales_month::date) as sales_year\n",
    ", kind_of_business\n",
    ", sum(sales) as sales\n",
    "from retale_sales rs \n",
    "where kind_of_business in ('Book stores', 'Sporting goods stores','Hooby, toy, and game stores')\n",
    "group by 1,2\n",
    "order by 1,2;\n",
    "\n",
    "-- 여성 의류업과 남성 의류업의 월간 매출\n",
    "select sales_month\n",
    ",kind_of_business\n",
    ",sales\n",
    "from retale_sales rs \n",
    "where kind_of_business in ('Men''s clothing stores' 'Women''s clothing stores')\n",
    "order by 1,2;\n",
    "\n",
    "-- 여성 의류업과남성 의류업의 연간 매출  \n",
    "select date_part('year', sales_month::date)\n",
    ",kind_of_business\n",
    ",sum(sales) as sales\n",
    "from retale_sales rs \n",
    "where kind_of_business in ('Men''s clothing stores', 'Women''s clothing stores')\n",
    "group by 1,2\n",
    "order by 1,2;\n",
    "\n",
    "-- 여성 의류업과 남성 의류업의 연간 매출 차이\n",
    "select date_part('year', sales_month::date) as sales_year\n",
    ",sum(case when kind_of_business = 'Women''s clothing stores' then sales end)\n",
    " - sum(case when kind_of_business = 'Men''s clothing stores' then sales end) as womens_minus_mens\n",
    "from retale_sales rs \n",
    "where kind_of_business in ('Men''s clothing stores', 'Women''s clothing stores') and sales_month <='2019-12-01'\n",
    "group by 1\n",
    "order by 1;\n",
    "\n",
    "-- 남성 의루업 대비 여성 의류업의 연간 매출 비율\n",
    "select sales_year\n",
    ",womens_sales / mens_sales as womens_times_of_mens\n",
    "from \n",
    "(\n",
    "\tselect date_part('year', sales_month::date) as sales_year\n",
    "\t,sum(case when kind_of_business='Women''s clothing stores' then sales end) as womens_sales\n",
    "\t,sum(case when kind_of_business='Men''s clothing stores' then sales end) as mens_sales\n",
    "\tfrom retale_sales rs\n",
    "\twhere kind_of_business in ('Men''s clothing stores', 'Women''s clothing stores')\n",
    "\tand sales_month <= '2019-12-01'\n",
    "\tgroup by 1\n",
    ") a \n",
    "order by 1;\t\n",
    "\n",
    "-- 남성 의루업 대비 여성 의류업의 연간 매출 비율차\n",
    "-- * yyyy연도, 여성 의류업 매출은 남성 의류업 매출보다 'womens_pcf_of_men'% 만큼 크다. 'womens_pcf_of_men'%/100 배다.\n",
    "select sales_year\n",
    ",(womens_sales / mens_sales -1)*100 as womens_pcf_of_mens\n",
    "from \n",
    "(\n",
    "\tselect date_part('year', sales_month::date) as sales_year\n",
    "\t,sum(case when kind_of_business='Women''s clothing stores' then sales end) as womens_sales\n",
    "\t,sum(case when kind_of_business='Men''s clothing stores' then sales end) as mens_sales\n",
    "\tfrom retale_sales rs\n",
    "\twhere kind_of_business in ('Men''s clothing stores', 'Women''s clothing stores')\n",
    "\tand sales_month <= '2019-12-01'\n",
    "\tgroup by 1\n",
    ") a \n",
    "order by 1;\t\n",
    "\n",
    "-- 여성 의류업과 남성 의류업의 월간 매출 비율\n",
    "select sales_month, kind_of_business, sales\n",
    ",sum(sales) over (partition by sales_month) as total_sales\n",
    ",sales*100 / sum(sales) over (partition by sales_month) as pct_total\n",
    "from retale_sales rs \n",
    "where kind_of_business in ('Men''s clothing stores', 'Women''s clothing stores')\n",
    "order by 1,2;\n",
    "\n",
    "-- 여성 의류업과 남성 의류업의 연간 매출 대비 월간 매출 비율\n",
    "select sales_month, kind_of_business, sales\n",
    ",sum(sales) over (partition by date_part('year', sales_month::date), kind_of_business) as yearly_sales\n",
    ",sales*100/\n",
    " sum(sales) over (partition by date_part('year', sales_month::date), kind_of_business) as pct_yearly\n",
    "from retale_sales rs \n",
    "where kind_of_business in ('Men''s clothing stores', 'Women''s clothing stores')\n",
    "order by 1,2;\n",
    "\n",
    "-- 첫해연도 매출기준으로 인덱싱된 남성 의류업매출과 여성 의류업 매출변화\n",
    "select sales_year, kind_of_business, sales\n",
    ",(sales/first_value(sales) over(partition by kind_of_business order by sales_year) -1)*100 as pct_from_index\n",
    "from \n",
    "(\n",
    "\tselect date_part('year', sales_month::date) as sales_year\n",
    "\t,kind_of_business\n",
    "\t,sum(sales) as sales\n",
    "\tfrom retale_sales rs \n",
    "\twhere kind_of_business in ('Men''s clothing stores', 'Women''s clothing stores')\n",
    "\tand sales_month <='2019-12-31'\n",
    "\tgroup by 1,2\n",
    ") a\n",
    "order by 1,2;\n",
    "\n",
    "-- 여성의류업의 월간 매출과 12개월 이동 평균 매출\n",
    "select a.sales_month\n",
    ",a.sales\n",
    ",avg(b.sales) as moving_avg\n",
    ",count(b.sales) as records_count\n",
    "from retale_sales a\n",
    "join retale_sales b on a.kind_of_business = b.kind_of_business \n",
    "and b.sales_month::date between a.sales_month::date - interval '11 months' and a.sales_month::date\n",
    "where a.kind_of_business = 'Women''s clothing stores' \n",
    "\tand b.kind_of_business = 'Women''s clothing stores'\n",
    "\tand a.sales_month >= '1993-01-01'\n",
    "group by 1,2\n",
    "order by 1;\n",
    "\n",
    "-- 여성 의류업의 월간매출과 누적매출\n",
    "select sales_month, sales\n",
    ",sum(sales) over (partition by date_part('year', sales_month::date) order by sales_month) as sales_ytd\n",
    "from retale_sales rs \n",
    "where kind_of_business = 'Women''s clothing stores' ;\n",
    "\n",
    "-- 서점업 매출의 전월 대비 증감율\n",
    "select a.sales_year, a.yearly_sales\n",
    ",lag(a.yearly_sales) over (order by a.sales_year) as prev_year_sales\n",
    ",(a.yearly_sales/lag(a.yearly_sales) over (order by a.sales_year) -1)*100 as pct_growth_from_previous -- 증감\n",
    "from (\n",
    "\tselect date_part('year', sales_month::date) as sales_year\n",
    "\t,sum(sales) as yearly_sales\n",
    "\tfrom retale_sales rs \n",
    "\twhere kind_of_business='Book stores'\n",
    "\tgroup by 1\n",
    ") a;\n",
    "\n",
    "-- 서점업의 매출, 전년 대비 매출 절대 차이, 전년대비 매출 비율\n",
    "select sales_month, sales -- 매\n",
    ",sales-lag(sales) over (partition by date_part('month', sales_month::date) order by sales_month) as absolute_diff -- 전년대비 매출절대차\n",
    ",(sales/lag(sales) over (partition by date_part('month', sales_month::date) order by sales_month) -1)*100 as pcf_diff -- 전년대비 매출비\n",
    "from retale_sales rs \n",
    "where kind_of_business = 'Book stores' ;\n",
    "\n",
    "-- 서점업 월간 매출 (1992 ~ 1994년)\n",
    "-- 예제 코드에서는 max집계 함수를 사용하지만, 분석은 sum, count등 집계함수를 사\n",
    "select date_part('month', sales_month::date) as month_number\n",
    ", to_char(sales_month::date, 'Month') as month_name\n",
    ", max(case when date_part('year', sales_month::date) = 1992 then sales end) as sales_1992\n",
    ", max(case when date_part('year', sales_month::date) = 1993 then sales end) as sales_1993\n",
    ", max(case when date_part('year', sales_month::date) = 1994 then sales end) as sales_1994\n",
    "from retale_sales rs \n",
    "where kind_of_business = 'Book stores'\n",
    "and sales_month between '1992-01-01' and '1994-12-01'\n",
    "group by 1,2;"
   ]
  }
 ],
